extend repoNavigation
block append content
  each val, index in commits
    .panel.panel-default
      .panel-heading
        h3.panel-title
          = index
      .panel-body
        .list-group
          each com, treeish in val
            .list-group-item
              p.list-group-item-text
                strong= com.msg
              p.list-group-item-text
                a(href='mailto:' + com.author.email)
                  = com.author.name
                = ' authored on ' + com.date
